Daily.bulk.summary = read.csv("ready_to_use/COVID-19/dateWise_bulk_summary.csv")
#---------------------------------------------------------------#
### Functions ###
plot.a.country = function(cName, yesORno = FALSE) {
Daily.bulk.summary.country = Daily.bulk.summary[
which(str_detect(Daily.bulk.summary$Country,
cName,
negate = yesORno)),
]
############################################
d <- Daily.bulk.summary.country %>%
as_tibble()
d_ends <- Daily.bulk.summary.country %>%
group_by(Country) %>%
top_n(1, Confirmed) %>%
pull(Deaths)    # col can be changed
d %>%
ggplot(aes(Confirmed, Deaths, color = Country)) +
geom_line(size = 2, alpha = .8) +
theme_minimal() +
scale_x_continuous(expand = c(0, 0)) +
scale_y_continuous(sec.axis = sec_axis(~ ., breaks = d_ends)) +
ggtitle("Few(specify type of such countries) reporting more deaths for the confirmed cases",
subtitle = "Based on the dataset of given country") +
labs(x = "Confirmed Cases", y = "Deaths", caption = "Plot by @ravi")
}
